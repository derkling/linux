CONFIG_CPU_FREQ=y

CONFIG_ARM_BIG_LITTLE_CPUFREQ=y
CONFIG_ARM_DT_BL_CPUFREQ=y

CONFIG_ARM_EXYNOS4X12_CPUFREQ=n
CONFIG_ARM_EXYNOS5440_CPUFREQ=n

CONFIG_CPU_FREQ_GOV_PERFORMANCE=y
CONFIG_CPU_FREQ_GOV_POWERSAVE=y
CONFIG_CPU_FREQ_GOV_USERSPACE=y

# on-demand disabled until cpufreq driver stabilizies 
# (otherwise ubuntu/debian userspace will always switch to it)
CONFIG_CPU_FREQ_GOV_ONDEMAND=n

CONFIG_CPU_FREQ_DEFAULT_GOV_USERSPACE=y
#CONFIG_CPU_FREQ_DEFAULT_GOV_PERFORMANCE=y

# Regulators for CPU voltage scaling
CONFIG_MFD_MAX77686=y
CONFIG_REGULATOR_MAX77802=y
